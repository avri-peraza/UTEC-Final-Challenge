            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def token = vars.get(&quot;token&quot;)
def responseCode = prev.getResponseCode()

log.info(&quot;&gt;&gt;&gt; Verificando extracci√≥n...&quot;)
log.info(&quot;&gt;&gt;&gt; Response Code: &quot; + responseCode)
log.info(&quot;&gt;&gt;&gt; Token extra√≠do: &quot; + (token != null ? token.substring(0, Math.min(20, token.length())) : &quot;NULL&quot;))

// Verificar si se obtuvo el token
if (token != null &amp;&amp; 
    token != &quot;TOKEN_NOT_FOUND&quot; &amp;&amp; 
    token != &quot;&quot; &amp;&amp;
    responseCode == &quot;200&quot;) {
    
    vars.put(&quot;loginSuccess&quot;, &quot;true&quot;)
    log.info(&quot;‚úÖ LOGIN EXITOSO - Token obtenidos&quot;)
} else {
    
    vars.put(&quot;loginSuccess&quot;, &quot;false&quot;)
    log.warn(&quot;‚ùå LOGIN FALLIDO&quot;)
    log.warn(&quot;‚ùå Token: &quot; + token)
    log.warn(&quot;‚ùå Response Code: &quot; + responseCode)
}

log.info(&quot;&gt;&gt;&gt; Verificaci√≥n final: loginSuccess = &quot; + vars.get(&quot;loginSuccess&quot;))


// Verificar que se guard√≥ correctamente
log.info(&quot;&gt;&gt;&gt; Verificaci√≥n final: loginSuccess = &quot; + vars.get(&quot;loginSuccess&quot;))</stringProp>
          </JSR223PostProcessor>
